{
  "manifest_version": 3,
  "name": "Surfline",
  "version": "1.0",
  "description": "Surfline Webcam",
  "content_scripts": [
    {
      "matches": [
        "https://www.surfline.com/surf-report/*?camId=*"
      ],
      "js": ["jquery.min.js", "content.js"],
      "run_at": "document_end"
    }
  ]
}
